<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content">

	<Ninja.coder.Ghostemane.code.Keyboard.Keyboard_View
			android:id="@+id/KeyboardView"
			android:background="#FF1B1B1F"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:keyBackground="@drawable/keyportback"
			android:shadowColor="#00000000"
			android:keyTextColor="#FFF3EFF4"
			android:keyPreviewLayout="@layout/preview_google"
			android:popupLayout="@layout/popup_google"/>

</LinearLayout>

